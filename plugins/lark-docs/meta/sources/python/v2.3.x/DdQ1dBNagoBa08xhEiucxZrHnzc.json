      {
        "block_id": "TV4mdXcXZohyhfx9i3Tc6KccnWg",
        "block_type": 14,
        "code": {
          "elements": [
            {
              "text_run": {
                "content": "from pymilvus import MilvusClient\n\n# 1. Create a milvus client\nclient = MilvusClient(\n    uri=\"http://localhost:19530\",\n    token=\"root:Milvus\"\n)\n\n# 2. Create a collection\nclient.create_collection(collection_name=\"test_collection\", dimension=5)\n\n# 3. Create a partition\nclient.create_partition(\n    collection_name=\"test_collection\", \n    partition_name=\"partition_A\"\n)\n\n# 4. Release the collection\nclient.release_collection(collection_name=\"test_collection\")\n\n# 5. Load a partition\nclient.load_partitions(\n    collection_name=\"test_collection\",\n    partition_names=[\"partition_A\"]\n)\n\n# 6. Check the load status of the collection\nclient.get_load_state(collection_name=\"test_collection\") \n\n# {'state': <LoadState: NotLoad>}\n\n# 7. Check the load status of the partition\nclient.get_load_state(\n    collection_name=\"test_collection\",\n    partition_names=[\"partition_A\"],\n)\n\n",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": "# {'state': <LoadState: Loaded>}",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            }
          ],
          "style": {
            "language": 49,
            "wrap": false
          }
