      {
        "block_id": "LaIHdEHevouSB0xNvwHc3oA9n6d",
        "block_type": 14,
        "code": {
          "elements": [
            {
              "text_run": {
                "content": "from pymilvus import MilvusClient\n\n# 1. Set up a milvus client\nclient = MilvusClient(\n    uri=\"http://localhost:19530\",\n    token=\"root:Milvus\"\n)\n\n# 2. Create a collection\nclient.create_collection(collection_name=\"quick_setup\", dimension=5)\n\n# 3. Check the load status of the collection\nclient.get_load_state(collection_name=\"quick_setup\") \n\n# {'state': <LoadState: Loaded>}\n\n# 4. Release the collection\nclient.release_collection(collection_name=\"quick_setup\")\nclient.get_load_state(collection_name=\"quick_setup\") \n\n# {'state': <LoadState: NotLoaded>}\n\n# 5. Create a partition\nclient.create_partition(\n    collection_name=\"quick_start\", \n    partition_name=\"partition_A\"\n)\n\n# 6. Load a partition\nclient.load_partitions(\n    collection_name=\"quick_start\",\n    partition_names=[\"partition_A\"]\n)\n\nclient.get_load_state(collection_name=\"quick_setup\") \n\n# {'state': <LoadState: NotLoaded>}\n\nclient.get_load_state(\n    collection_name=\"quick_setup\",\n    partition_name=\"partition_A\"\n) \n\n",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": "# {'state': <LoadState: Loaded>}",
                "text_element_style": {
                  "bold": false,
