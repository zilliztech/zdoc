      {
        "block_id": "doxcnkfDo7EgrFsMCArsYu3GvFh",
        "block_type": 14,
        "code": {
          "elements": [
            {
              "text_run": {
                "content": "from pymilvus import DataType, FieldSchema, CollectionSchema\n\n# Define fields and create a schema\nprimary_key = FieldSchema(\n    name=\"id\",\n    dtype=DataType.INT64,\n    is_primary=True,\n)\n\nvector = FieldSchema(\n    name=\"vector\",\n    dtype=DataType.FLOAT_VECTOR,\n    dim=768,\n)\n\n# Create dictionary representation \nschema_dict = {\n    \"fields\": [     \n        primary_key.to_dict(),\n        vector.to_dict()                \n    ]\n}  \n\n# Reconstruct schema from dictionary \nschema = CollectionSchema.construct_from_dict(schema_dict)  \n# schema is now a CollectionSchema instance reconstructed from the dictionary \nprint(schema)\n\n# Output\n",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": "# {'auto_id': False, 'description': '', 'fields': [{'name': 'id', 'description': '', 'type': <DataType.INT64: 5>, 'is_primary': True, 'auto_id': False}, {'name': 'vector', 'description': '', 'type': <DataType.FLOAT_VECTOR: 101>, 'params': {'dim': 768}}]}",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            }
