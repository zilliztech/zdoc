                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": " dimension=5",
                "text_element_style": {
                  "bold": false,
                  "comment_ids": [
                    "7330925055448858627"
                  ],
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": "\n)\n\n# 3. Refresh the load status of the collection\nclient.refresh_load(\n    collection_name=\"test_collection\"\n",
