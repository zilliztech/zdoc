          "elements": [
            {
              "text_run": {
                "content": "Cost-Optimized Compute Unit (CU)",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
