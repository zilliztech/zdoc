        "block_type": 2,
        "parent_id": "KbxTdLFppoDxuIxSiv4cCLIpnF8",
        "text": {
          "elements": [
            {
              "text_run": {
                "content": "Zilliz Cloud 2.0.0 introduces the Partition Key feature. This feature allows you to designate a specific field as a partition key when creating a collection. Entities are then stored into partitions based on the values of their partition keys.",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
