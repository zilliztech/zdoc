                "content": "RBAC",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": " support, partition key, dynamic schema, and ",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
