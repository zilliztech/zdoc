            "folded": false
          }
        }
      },
      {
        "block_id": "J9fhd9zdborwngxyGLwc2qMYngb",
        "block_type": 14,
        "code": {
          "elements": [
            {
              "text_run": {
                "content": "embeddings = OpenAIEmbeddings()\nconnection_args = { 'uri': URI, 'token': TOKEN }\n\nvector_store = Zilliz(\n    embedding_function=embeddings, \n    connection_args=connection_args,\n    collection_name=COLLECTION_NAME,\n    drop_old=True,\n).from_documents(\n    all_splits,\n    embedding=embeddings,\n    collection_name=COLLECTION_NAME,\n    connection_args=connection_args,\n",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": ")",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            }
          ],
          "style": {
