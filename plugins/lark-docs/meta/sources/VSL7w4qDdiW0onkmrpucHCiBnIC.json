        "text": {
          "elements": [
            {
              "text_run": {
                "content": "After preparing the documents, the next step is to convert them into vector embeddings and save them in the vector store.",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            }
          ],
          "style": {
