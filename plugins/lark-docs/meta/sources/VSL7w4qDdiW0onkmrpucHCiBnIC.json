        "text": {
          "elements": [
            {
              "text_run": {
                "content": "The following code snippet sets up a RAG chain using OpenAI as the LLM and a RAG prompt.",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
