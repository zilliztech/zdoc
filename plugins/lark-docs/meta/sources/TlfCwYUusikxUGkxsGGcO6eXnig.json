        "parent_id": "FbIPdV2BpoTuwux5N8gc4qxmncb",
        "text": {
          "elements": [
            {
              "text_run": {
                "content": "Currently, Zilliz Cloud supports three types of similarity Metrics: ",
                "text_element_style": {
