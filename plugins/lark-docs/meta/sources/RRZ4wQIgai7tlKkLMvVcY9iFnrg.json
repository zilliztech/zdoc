        "block_type": 12,
        "bullet": {
          "elements": [
            {
              "text_run": {
                "content": "The schema and the index parameters.",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            }
          ],
