        "block_type": 2,
        "parent_id": "P81ndEZ41o1OTYxqK75cEg0MnVh",
        "text": {
          "elements": [
            {
              "text_run": {
                "content": "As explained in ",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "mention_doc": {
