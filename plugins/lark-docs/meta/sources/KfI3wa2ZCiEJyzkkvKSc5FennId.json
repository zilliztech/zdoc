        "block_type": 12,
        "bullet": {
          "elements": [
            {
              "text_run": {
                "content": "Boolean value (BOOLEAN)",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
