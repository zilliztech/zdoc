          "elements": [
            {
              "text_run": {
                "content": "Schema",
                "text_element_style": {
                  "bold": false,
                  "comment_ids": [
                    "7307443156269088772"
                  ],
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
