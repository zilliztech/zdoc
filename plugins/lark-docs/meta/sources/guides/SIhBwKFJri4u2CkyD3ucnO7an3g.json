        "text": {
          "elements": [
            {
              "text_run": {
                "content": "To safeguard data integrity, Zilliz Cloud offers reliable backup and restoration mechanisms.",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
