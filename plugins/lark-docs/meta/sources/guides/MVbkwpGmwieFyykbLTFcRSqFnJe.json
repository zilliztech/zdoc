        "heading2": {
          "elements": [
            {
              "text_run": {
                "content": "How does the connector work?",
                "text_element_style": {
                  "bold": false,
                  "comment_ids": [
                    "7350585264165158916"
                  ],
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
