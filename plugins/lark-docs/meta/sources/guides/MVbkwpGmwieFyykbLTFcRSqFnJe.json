          }
        }
      },
      {
        "block_id": "WXE7dNrpLoKoWzxoGSccLs75ndb",
        "block_type": 12,
        "bullet": {
          "elements": [
            {
              "text_run": {
                "content": "User",
                "text_element_style": {
                  "bold": true,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": ": Users can access the current token usage via the control platform.",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            }
          ],
          "style": {
            "align": 1,
