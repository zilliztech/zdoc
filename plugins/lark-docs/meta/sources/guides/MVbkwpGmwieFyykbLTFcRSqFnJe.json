        "block_type": 4,
        "heading2": {
          "elements": [
            {
              "text_run": {
                "content": "What is a connector?",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            }
