        "text": {
          "elements": [
            {
              "text_run": {
                "content": "The connector consists of four components.",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
