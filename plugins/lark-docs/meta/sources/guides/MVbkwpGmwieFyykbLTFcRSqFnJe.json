          }
        },
        "parent_id": "JiOIdARjCoNjItxamDBcD3gTnid"
      },
      {
        "block_id": "B3DKdi774oZ18nxgnthcpOaqnqh",
        "block_type": 2,
        "parent_id": "JiOIdARjCoNjItxamDBcD3gTnid",
        "text": {
          "elements": [
            {
              "text_run": {
                "content": "When a connector is created, Zilliz Cloud periodically checks the provided object storage path for file addition or deletion. The workflow diagram below illustrates this process.",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            }
          ],
          "style": {
            "align": 1,
            "folded": false
          }
        }
      },
