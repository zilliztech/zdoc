      {
        "block_id": "SyySdTbuYoNhbQxtVb6caXYZnpd",
        "block_type": 2,
        "parent_id": "XZSbd5bksofEB0xTIHjc1oMRnYg",
        "text": {
          "elements": [
            {
              "text_run": {
                "content": "Zilliz Cloud Pipelines is a robust solution for transforming unstructured data such as documents, text pieces and images into a searchable vector ",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": "collection",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "link": {
                    "url": "https%3A%2F%2Fzilliverse.feishu.cn%2Fwiki%2FKfI3wa2ZCiEJyzkkvKSc5FennId%23Ht5adrJhToY5sJxb9KtcZKuYn7f"
                  },
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": ". This guide provides a detailed description of the three main Pipelines types and their functions.",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            }
          ],
          "style": {
            "align": 1,
            "folded": false
          }
        }
      },
      {
