        "text": {
          "elements": [
            {
              "text_run": {
                "content": "To achieve these consistency levels, Zilliz Cloud employs the concept of the ",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
