      },
      {
        "block_id": "MPrkdju6XoKsCJx8u1LcLJ0Mnyc",
        "block_type": 2,
        "parent_id": "Cxd3doY5Yo1ByUxTHJucOvgcnrh",
        "text": {
          "elements": [
            {
              "text_run": {
                "content": "In these next few steps we will:",
                "text_element_style": {
