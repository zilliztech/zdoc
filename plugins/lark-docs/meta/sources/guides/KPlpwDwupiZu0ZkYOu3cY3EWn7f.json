      },
      {
        "block_id": "KVBydEyH6oe3ZoxFnDccZ61knqD",
        "block_type": 14,
        "code": {
          "elements": [
            {
              "text_run": {
                "content": "# Parameters for set up Zilliz Cloud\nCOLLECTION_NAME = 'movies_db'  # Collection name\nDIMENSION = 384  # Embeddings size\nURI = 'YOUR_CLUSTER_ENDPOINT'  # Endpoint URI obtained from Zilliz Cloud\nTOKEN = 'YOUR_CLUSTER_TOKEN'  # API key or a colon-separated cluster username and password\n\n# Inference Arguments\nBATCH_SIZE = 128\n\n# Search Arguments\n",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": "TOP_K = 3",
