      {
        "block_id": "ENAvdDT5Toclugx2B1ucjhcbnwy",
        "block_type": 2,
        "parent_id": "Cxd3doY5Yo1ByUxTHJucOvgcnrh",
        "text": {
          "elements": [
            {
              "text_run": {
                "content": "For this example, we are going to use the SentenceTransformers miniLM model to create embeddings of the plot text. This model returns 384-dimensional embeddings.",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
