      {
        "block_id": "BwzDdegoHoRlqyxIaxEcqw5JnCb",
        "block_type": 14,
        "code": {
          "elements": [
            {
              "text_run": {
                "content": "import gdown\n\nurl = 'https://drive.google.com/uc?id=11ISS45aO2ubNCGaC3Lvd3D7NT8Y7MeO8'\nzipball = '../movies.zip'\noutput_folder = '../movies'\ngdown.download(url, zipball)\n\nwith zipfile.ZipFile(zipball,\"r\") as zip_ref:\n",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": "    zip_ref.extractall(output_folder)",
                "text_element_style": {
                  "bold": false,
